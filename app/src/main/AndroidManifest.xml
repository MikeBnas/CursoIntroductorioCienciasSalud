<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wordpress.medicourses.cursointroductoriocienciassalud">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.CursoIntroductorioCienciasSalud">
        <activity android:name=".ValidateActivity"></activity>
        <activity android:name=".Anatomia" />
        <activity android:name=".MediCourses" />
        <activity android:name=".PDFActivity" />
        <activity android:name=".Publica" />
        <activity android:name=".Biologia" />
        <activity android:name=".Quimica" />
        <activity android:name=".Fisica" />
        <activity android:name=".Matematicas" />
        <activity android:name=".Lenguaje" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>